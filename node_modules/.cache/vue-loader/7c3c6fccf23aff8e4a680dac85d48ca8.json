     </div>\n              <div class=\"span2\">小区电话: &nbsp;&nbsp;&nbsp; {{videoMessage.iphone}}</div>\n            </div>\n\n            <div class=\"videoList-button\">\n              <div class=\"videoList-button-div1\">\n                平均在刷率: &nbsp; <span>90</span>%\n              </div>\n              <div class=\"videoList-button-div2\">视频记录</div>\n            </div>\n          </div>\n        </div> \n      </div> \n    </div>   \n  </div>\n</template>\n\n<script>\nimport 'video.js/dist/video-js.css'\nimport 'vue-video-player/src/custom-theme.css'\n// import 'videojs-contrib-hls.js/src/videojs.hlsjs'\nexport default {\n  name: \"seamless\",\n  components: {\n  },\n  data() {\n    return {\n      videoMessage:{\n        location:\"卢浮公馆\",\n        videoUrl:\"\",\n        iphone:\"0532-8876532\",\n      },\n      videoUrl: require('./12.mp4'),//视频地址\n    }\n  },\n  computed: {\n  },\n  mounted() {\n    this.initVideo();\n  },\n  methods: {\n    initVideo() {\n      let myPlayer = this.$video(myVideo, {\n        controls: true,\n        autoplay: true,\n        //是否静音播放\n        muted:true,\n        //是否流体自适应容器宽高\n        fluid:true,\n        });\n      }\n  },\n  beforeDestroy() {\n    \n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.rightTop {\n  left: 1370px;\n  top: 110px;\n  position: absolute;\n\n  width: 500px; \n  height: 450px;\n  background:url(../../assets/rightTop.png) no-repeat;\n  background-size: 100% 100%;\n  overflow: visible;\n  .videoList {\n    position: absolute;\n    height: 100%;\n    width: 100%;\n    display: flex;\n    // justify-content: center;\n    align-items: center;\n    flex-direction: column;\n    padding-top: 15px;\n    &-play{\n      width: 80%;\n      height: 75%;\n      .span1{\n        font-size: 18px;\n        color: white;\n        margin-bottom: 6px;\n      }\n      .span2{\n        font-size: 18px;\n        color: white;\n        margin-top: 12px;\n      }\n    }\n    &-button{\n      width: 80%;\n      height: 20%;\n      color: white;\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      &-div1{\n        font-size: 18px;\n        span{\n          margin: 0;\n          padding: 0;\n          font-size: 22px;\n          width: 22px;\n          height: 22px;\n        }\n      }\n      &-div2{\n        width: 30%;\n        height: 50%;\n        text-align: center;\n        font-size: 18px;\n        color: white;\n        line-height: 48px;\n        box-shadow: 0 0 15px rgb(58, 143, 185) inset;\n      }\n    }\n  }\n}\n</style>\n"]}]}